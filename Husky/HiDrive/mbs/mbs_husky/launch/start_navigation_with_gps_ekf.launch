<launch>
    
    <!-- Run navsat gps to odometry conversion-->
  <include file="$(find mbs_husky)/launch/include/start_navsat.launch" />

    
    <!-- Run the ekf for map to odom config -->
    <node pkg="robot_localization" type="ekf_localization_node" name="ekf_localization_with_gps"> 
      <rosparam command="load" file="$(find mbs_husky)/config/robot_localization_with_gps.yaml" />
      <!-- <remap from="/odometry/filtered" to="/odometry/filtered/global" /> -->
    </node>


    <!-- Run the map server -->
    <arg name="map_file" default="$(find mbs_husky)/maps/mymap_empty.yaml"/>
    <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />
    
    <!--- Run Move Base -->
    <!-- <include file="$(find mbs_husky)/launch/include/with_map_move_base.launch" /> -->
    <!-- <include file="$(find mbs_husky_navigation)/launch/gps_move_base.launch" /> -->
 

    <!-- Start RVIZ for Localization -->
    <include file="$(find mbs_husky)/launch/include/view_robot.launch">
        <arg name="config" value="localization" />
    </include>

</launch>